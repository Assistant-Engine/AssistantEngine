@using System.Reflection
@inherits LayoutComponentBase
@{
 var asm = Assembly.GetEntryAssembly()?.GetName().Name ?? typeof(App).Assembly.GetName().Name;
}



<!-- Prism core & theme -->

<HeadContent>
   

    <link rel="stylesheet" href="/@(asm)/@(asm).styles.css?v=dev" />
    <link rel="stylesheet" href="/@(asm).styles.css" /> 
    <link rel="stylesheet" href="/AssistantEngine.Web.styles.css" />
 
    <link rel="stylesheet" href="/_content/AssistantEngine.UI/app.css" />
    @**<link rel="stylesheet" href="/_content/AssistantEngine.UI/AssistantEngine.UI.styles.css" />*@
    
    <link rel="stylesheet" href="/_content/AssistantEngine.UI/lib/toastr/toastr.min.css" />
    <link rel="stylesheet" href="/_content/AssistantEngine.UI/lib/prism/prism.css" />
    <link href="/_content/AssistantEngine.UI/icons/css/uicons-bold-rounded.css" rel="stylesheet">

    <script src="/_content/AssistantEngine.UI/lib/jquery/jquery.js"></script>
    <script src="/_content/AssistantEngine.UI/lib/toastr/toastr.min.js"></script>
    <script src="/_content/AssistantEngine.UI/lib/prism/prism.js"></script>
    <script src="/_content/AssistantEngine.UI/dropdown.js"></script>
    <script type="module" src="/_content/AssistantEngine.UI/app.js"></script>
</HeadContent>

<div class="main-content" id="main-content">
    @Body
</div>
<script src="_framework/blazor.web.js"></script>
<div id="blazor-error-ui" data-nosnippet>
    An unhandled error has occurred.
    <a href="." class="reload">Reload</a>
    <span class="dismiss">🗙</span>
</div>
